/* eslint-disable global-require */
/* eslint-disable import/no-extraneous-dependencies */

import React from 'react';

// We create this wrapper so that we only import react-hot-laoder for a
// development build.  Small savings. :)
var ReactHotLoader = process.env.NODE_ENV === 'development' ? require('react-hot-loader').AppContainer : function (_ref) {
  var children = _ref.children;
  return React.Children.only(children);
};

var _default = ReactHotLoader;
export default _default;
;

var _temp = function () {
  if (typeof __REACT_HOT_LOADER__ === 'undefined') {
    return;
  }

  __REACT_HOT_LOADER__.register(ReactHotLoader, 'ReactHotLoader', '/Users/Adam/Dev/listman/client/components/ReactHotLoader.js');

  __REACT_HOT_LOADER__.register(_default, 'default', '/Users/Adam/Dev/listman/client/components/ReactHotLoader.js');
}();

;